<%- include('../header') %>
    <div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-12 d-flex no-block align-items-center">
                    <h4 class="page-title">Tables</h4>
                    <div class="ms-auto text-end">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">
                                    Library
                                </li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- End Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="d-flex justify-content-between">
                                <h5 class="card-title">Table</h5>
                                <a href="/exsubcategory/add" class="btn btn-success">Add</a>

                            </div>
                        </div>
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Category</th>
                                    <th scope="col">Subcategory</th>
                                    <th scope="col">Exsubcategory</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">action</th>


                                </tr>
                            </thead>
                            <tbody>
                                <% exsubcategory.map((exsub,i)=>{ %>
                                    <tr>
                                        <th scope="row">
                                            <%= ++i %>
                                        </th>
                                        <td>
                                            <%= exsub.categoryId?.category %>
                                        </td>
                                        <td>
                                            <%= exsub.subcategoryId?.subcategory %>
                                        </td>
                                        <td>
                                            <%= exsub.exsubcategory %>
                                        </td>
                                        <td>
                                            <a href="/exsubcategory/changeStatus?status=<%= exsub.status %>&id=<%= exsub.id %>"
                                                class="<% if(exsub.status == `active`) { %> btn
                                                btn-success <% } else { %> btn btn-danger <% } %>">
                                                <%= exsub.status %>
                                            </a>
                                        </td> 
                                         <td>
                                            <a href=" /exsubcategory/deleteexsubCategory?id=<%=exsub.id %>"
                                                class="btn btn-danger">delete
                                                
                                            </a>
                                            <a href="/exsubcategory/editexsubCatagory?id=<%= exsub.id %>" class="btn btn-primary">Edit</a>
                                        </td>
                                    </tr>
                                    <% }) %>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End PAge Content -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Right sidebar -->
            <!-- ============================================================== -->
            <!-- .right-sidebar -->
            <!-- ============================================================== -->
            <!-- End Right sidebar -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- End Container fluid  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
        <footer class="footer text-center">
            All Rights Reserved by Matrix-admin. Designed and Developed by
            <a href="https://www.wrappixel.com">WrapPixel</a>.
        </footer>
        <!-- ============================================================== -->
        <!-- End footer -->
        <!-- ============================================================== -->
    </div>

    <%- include('../footer') %>